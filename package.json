{
  "name": "yut",
  "description": "YouTube terminal music player",
  "private": true,
  "scripts": {
    "start:dev": "yarn ts-node index.ts",
    "build": "yarn run tsc -p ./tsconfig.json",
    "build:prod": "yarn run tsc -p ./tsconfig.json && cd dist yarn install --production",
    "build:watch": "yarn run tsc -w -p ./tsconfig.json"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "bresenham": "^0.0.4",
    "dotenv": "^8.2.0",
    "drawille": "^1.1.1",
    "electron": "^9.0.2",
    "googleapis": "^52.1.0",
    "howler": "^2.2.0",
    "tslib": "^2.0.0",
    "ytdl-core": "^2.1.7"
  },
  "devDependencies": {
    "@types/blessed": "0.1.17",
    "@types/bresenham": "^0.0.0",
    "@types/howler": "^2.1.2",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
